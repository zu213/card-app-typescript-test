// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./database.db"
}

model Entry {
  id          String   @id @default(uuid())
  title       String
  description String
  created_at  DateTime
  scheduled_for DateTime?
}
